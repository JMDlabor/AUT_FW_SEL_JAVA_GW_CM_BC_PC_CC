<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" parallel="methods"
	name="PolicyCenter_TestSuite">


	<listeners>
		<listener
			class-name="com.Utils.Selenium.Selenium_TestNG_TestListener" />
	</listeners>

	<parameter name="GW_Application" value="PolicyCenter" />

	<test thread-count="1" parallel="methods" name="AccountCreation">
		<classes>
			<class
				name="com.Test.Guidewire.PolicyCenter.GW_PC_AccountCreation" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="1" parallel="methods" name="NewSubmission">
		<classes>
			<class
				name="com.Test.Guidewire.PolicyCenter.GW_PC_NewSubmission" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
